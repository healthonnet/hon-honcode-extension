{
  "name": "__MSG_appName__",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/utils/md5.js",
      "scripts/utils/hash.js",
      "scripts/utils/listHON.js",
      "scripts/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>",
    "storage",
    "webRequest"
  ],
  "options_ui": {
    "chrome_style": true
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon-invalid-16.png",
      "48": "images/icon-invalid-48.png",
      "128": "images/icon-invalid-128.png"
    },
    "default_title": "HonCode"
  },
  "web_accessible_resources": [
    "styles/main.css",
    "images/**"
  ]
}
